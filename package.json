{
  "name": "default-number",
  "version": "0.1.0",
  "description": "Get a default number when a number is a non-number, a NaN or out of range.",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "coveralls": "nyc --reporter=text-lcov npm test | coveralls",
    "web:install": "npm i phantomjs-prebuilt mocha-phantomjs",
    "web:build": "browserify index.js --standalone defaultNumber | uglifyjs --compress --mangle -o web/default-number.js && node test/web/make.js",
    "web:test": "mocha-phantomjs -p node_modules/.bin/phantomjs test/web/default-number.test.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sttk/default-number.git"
  },
  "keywords": [
    "default",
    "number",
    "range",
    "check"
  ],
  "author": "Takayuki Sato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sttk/default-number/issues"
  },
  "homepage": "https://github.com/sttk/default-number#readme",
  "devDependencies": {
    "browserify": "^16.5.0",
    "chai": "^3.5.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.1.0",
    "mocha": "^3.2.0",
    "nyc": "^14.1.1",
    "uglify-js": "^3.6.0"
  },
  "dependencies": {
    "default-val": "^0.1.4"
  }
}
